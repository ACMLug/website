<script language=javascript>
function bVer(){
  this.ie=document.all?1:0
  this.ns=document.layers?1:0;
}
bn=new bVer()

function lObj(obj){									
  this.css=bn.ns?document[obj]:document.all[obj].style
  this.divMove=divMoveIt;
}

function hideIt() {
  if(bn.ns) {document.def.visibility='hidden';}
  else if(bn.ie) {document.all.def.style.visibility='hidden';}
}

function divMoveIt(y){
  this.y=y
  this.css.top=this.y
}

function rePos() {
if(bn.ie) {bg1.divMove(document.body.scrollTop);}
else if(bn.ns) {
  bg1.divMove(window.pageYOffset);
  t1=setTimeout("rePos()", 2)
  }
}

function init() {
  bg1=new lObj('def');
  if(bn.ns) {rePos();}
  else if(bn.ie) {window.onscroll=rePos;}
}

onload=init;
</script>
